@inproceedings{Khribi2007,
abstract = {The last decade has witnessed a great interest in e-learning and Web based education areas. Unfortunately, most of the e-learning environments used in the educational field today are still delivering the same educational resources and services in the same way to different learners. Hence, observing the increasing need for personalization in e-learning systems, we aim to make these systems deliver the most appropriate content to learners according to their interests and needs. This paper outlines the use of on-line automatic recommendations in e-learning systems based on learners’ access history. First we start by mining learner profiles using usage Web mining techniques and content-based profiles using information retrieval techniques. Then, we use these profiles to compute relevant links to recommend for an active learner by applying a number of recommendation strategies. },
annote = {S.2 oben:  Gute Einleitung f\"{u}r hypermedia paper, da Bezugnahme zur Entwicklung der Adaption (Unterscheidung: Conent vs. navigation) -> Monitoring User Activities, sowie interpretation sind wesentliche Bestandteile.  Dann: Beschreibung der Technologie f\"{u}r Recommender Systeme eingeteilt in: - content based filtering - collaborative filtering - hyprid approach for filtering  F\"{u}r den Content-Ansatz wird Nutch (Indexing) genutzt. collaborative bezieht sich hier auf die \"{U}bereinstimmung von Benutzerprofilen aufgrund von \"{a}hnlichen Sessions aus Web-Logs (Kalt-Start-Problematik!!!)   Zum Schluss werden zwei Hybrid-Ans\"{a}tze vorgestellt, die sich auf die Kombination von Content + Collaborative beziehen.},
author = {Khribi, Mohamed Kouthea\"{\i}r and Jemni, Mohamed and Nasraoui, Olfa},
