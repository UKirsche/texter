@book{Svolik2009,
abstract = {I examine the causes and dynamics of democratic consolidation. I develop a new, change-point model of democratic consolidation that avoids some of the pitfalls of many existing empirical studies of consolidation. Specifically, rather than assuming that consolidation occurs, the present model estimates whether and when consolidation occurs as a function of covariates. I apply this approach to new data on democratic survival that distinguish between two processes by which an overwhelming majority of authoritarian reversals occurs: coups d’´etat and incumbent takeovers. I find that the risk of authoritarian reversals by each of the two processes differs in its dynamic as well as determinants: democracies consolidate against the risk of coups but do not consolidate against the risk of incumbent takeovers. I discuss the theoretical implications of these empirical findings.},
address = {Champaign, IL},
annote = {Unpublished article ?},
author = {Svolik, Milan W},
keywords = {Cambodia},
title = {{When and Why Democracies Consolidate: Coups, Incumbent Takeovers, and Democratic Survival}},
year = {2009}
}
